import(Windows/Form)
import(Windows/SimpleLabel)
import(Windows/KeyEventArgs)
; using System.Windows.Forms

Form form becomes new(Form,"My Form",100,100,600,400)
; Form form = new Form("My Form",100,100,600,400);

SimpleLabel lbl becomes new(SimpleLabel)
; SimpleLabel lbl = new SimpleLabel();

lbl.text becomes "Hello world"
; lbl.text = "Hello world";

form.addComponent(lbl)
; form.Controls.Add(lbl);

func(KeyEventArgs args) keyDown {
	
	lbl.x becomes lbl.x+3
	form.rePaint
	
}

form.onKeyDown.addFunction($keyDown)

form.show
; form.show();
